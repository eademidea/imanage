{
  "name": "imanage",
  "version": "1.0.1",
  "description": "Manage my life",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev ./src/StartApp.ts",
    "test": "jest",
    "knex:migrate": "knex --knexfile ./src/database/knex/Environment.ts migrate:latest",
    "knex:rollback": "knex --knexfile ./src/database/knex/Environment.ts migrate:rollback",
    "knex:rollback-all": "knex --knexfile ./src/database/knex/Environment.ts migrate:rollback --all",
    "knex:seed": "knex --knexfile ./src/database/knex/Environment.ts seed:run"
  },
  "author": "Conrado J Oliveira",
  "license": "MIT",
  "repository": "https://github.com/eademidea/imanage.git",
  "private": false,
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "connect": "^3.2.0",
    "express": "^4.18.2",
    "js-yaml": "^3.3.0",
    "knex": "^2.4.2",
    "oas3-tools": "^2.2.3",
    "swagger": "^0.7.5",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.4.1",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
